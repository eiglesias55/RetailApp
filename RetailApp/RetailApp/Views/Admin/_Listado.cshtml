@{
    ViewBag.Title = "Listado";

}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>@ViewBag.Title</title>

    <!-- Icon -->
    <link rel="icon" href="../Content/Media/Img/icon_gA.png" type="image/png">

    <!-- Bootstrap -->
    <link href="@Url.Content("~/Content/Media/Bootstrap/css/bootstrap.min.css")" rel="stylesheet">
    <link href="@Url.Content("~/Content/Media/Bootstrap/css/bootstrap-theme.min.css")" rel="stylesheet">
    <link href="@Url.Content("~/Content/Media/css/carrousel.css")" rel="stylesheet">
    <link href="@Url.Content("~/Content/Media/css/Layout.css")" rel="stylesheet">
    <link href="@Url.Content("~/Content/Media/css/signin2.css")" rel="stylesheet" />

    <script src="../Content/Media/JQuery/jquery-2.1.3.min.js" type="text/javascript"></script>
    <script type="text/javascript" src="../Content/Media/Bootstrap/js/button_functions.js"></script>
    <script type="text/javascript" src="../Content/Media/Bootstrap/js/fbLogin.js"></script>

    <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
      <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
      <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
</head>
<body>
    <div class="container">
        <div class="col-xs-6">
            <h1>Listado de Usuarios</h1>
        </div>
        <div class="col-xs-6" id="buttonsAdmin">
            <div class="btn-group" role="group" aria-label="...">
                <button type="button" class="btn btn-default">Generar Token</button>
                <button type="button" class="btn btn-default">Estatus de Usuarios</button>
                <button type="button" class="btn btn-default">Enviar Email</button>
                <button type="button" class="btn btn-default">Re-enviar Email</button>
            </div>
        </div>
            <table class="table">
                <thead>
                    <tr>
                        <td>Token</td>
                        <td>Apellido</td>
                        <td>Nombre</td>
                        <td>Enviado</td>
                        <td>Recibido</td>
                    </tr>
                </thead>
                <tbody>
                    <tr>
                        <td>
                            asdfsdafsdafds
                            </td>
                        <td>
                            Leon
                        </td>
                        <td>
                            Sebastian
                        </td>
                        <td>no</td>
                        <td>no</td>
                    </tr>
                <tbody>
            </table>
    </div>
    <div style="margin-top:90px;">
        @RenderBody()
    </div>
</body>
</html>